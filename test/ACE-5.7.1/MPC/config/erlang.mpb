feature(erlang) {
  Define_Custom(Erlang) {
    automatic_in      = 1
    automatic_out     = 1
    command           = erlc
    commandflags      =
    inputext          = .erl, .yrl, .mib, .bin, .rel, .asn1, .idl
    generic_outputext = .beam, .jam, .erl, .hrl, .bin, .rel, .boot, .asn1db
    keyword erlcflags = commandflags
  }
}
